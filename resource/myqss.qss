QWidget {
    color: white;
    background-color: #363D3A;
}
QWidget#color {
    color: white;
    background-color: #177FA6;
}
QWidget#transparent {
    background-color: transparent;
}
.QWidget#user_info {
    border-image: url(:/default/default_pngs/notvip_bg_reck.png);
}
QPushButton:checked#title_menu {
    border: none;
    background-color: transparent;
    border-width:3px;
    border-style:none none solid none;
    border-color:#10bacb;
}
QPushButton:!checked#title_menu {
    border: none;
    background-color: transparent;
}
QPushButton#transparent {
    border: none;
    background-color: transparent;
}
QPushButton#link {
    border: none;
    color: green;
    background-color: transparent;
}
QPushButton#setting_button{
    border-image: url(':/default/default_pngs/setting_normal.png');
    background-color: transparent;
}
